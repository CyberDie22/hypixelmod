{
  "schemaVersion": 1,
  "id": "hypixelmod",
  "version": "${version}",

  "name": "Hypixel Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Ben Buzard"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/hypixelmod/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
        {
            "adapter": "kotlin",
            "value": "com.benbuzard.hypixelmod.HypixelModClient"
        }
    ]
  },
  "mixins": [
    "hypixelmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.12",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.9.0+kotlin.1.8.0"
  }
}
